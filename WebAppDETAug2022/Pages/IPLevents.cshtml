@page
@using WebAppDETAug2022.Models
@model WebAppDETAug2022.Pages.IPLeventsModel
@{
}
<h1>Welcome to IPL 2022</h1>


<h2>Ticket Details</h2>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Price</th>
        <th>Match </th>
    </tr>
    @foreach (Ticket t in Model.Tickets)
    {
        <tr>
            <td> @t.Id </td>
            <td> @t.Price </td>
            <td> @t.Match </td>
        </tr>
    }
</table>